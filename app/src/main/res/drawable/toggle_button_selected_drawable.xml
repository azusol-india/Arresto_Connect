<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <padding
                android:bottom="3dp"
                android:left="3dp"
                android:right="3dp"
                android:top="3dp" />
            <solid android:color="@color/white" />
            <stroke
                android:width="2dp"
                android:color="@color/dark_green" />
            <size
                android:width="17dp"
                android:height="17dp" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/app_green" />
            <size
                android:width="9dp"
                android:height="9dp" />
        </shape>
    </item>
</layer-list>